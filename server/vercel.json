{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key",
    "MONGODB_URI": "@mongodb-uri",
    "NODE_ENV": "production"
  }
}
